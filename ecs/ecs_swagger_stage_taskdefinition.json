{
  "containerDefinitions": [
    {
      "name": "swagger",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/ezasdf-swagger:stage",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ezasdf-swagger-stage",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "log-prefix"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "URL",
          "value": "swagger.json"
        }
      ],
      "memoryReservation": 300
    }
  ],
  "family": "ezasdf-swagger-stage-td"
}
